{
  "name": "@revolist/revogrid-column-numeral",
  "version": "2.0.8",
  "description": "Number column type for RevoGrid system based on numeraljs",
  "main": "./dist/revo-column-number.umd.cjs",
  "module": "./dist/revo-column-number.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/revo-column-number.mjs",
      "require": "./dist/revo-column-number.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack serve --config ./demo/webpack.config.js",
    "release": "npm run build && npm publish --public --tag pre-release"
  },
  "keywords": [],
  "author": "revolist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revolist/revogrid-column-numeral.git"
  },
  "bugs": {
    "url": "https://github.com/revolist/revogrid-column-numeral/issues"
  },
  "homepage": "https://github.com/revolist/revogrid-column-numeral#readme",
  "devDependencies": {
    "@revolist/revogrid": "^4.12.5",
    "@types/numeral": "^2.0.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "numeral": "^2.0.6"
  }
